                    </div>
                    <div class="carousel-item">
                      <img class="d-block w-100" src="img/slider1a.png" alt="First slide" id="slider3">
                    </div>
					<div  class="carousel-item">
					  <img  class="d-block w-100" src="img/slider4.png" alt="First slide" id="slider4">
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
			<!-- start Login box -->
			<div class="container" id="login-block">
				<div class="row">
					<div class="col-sm-6 col-md-4 col-sm-offset-3 col-md-offset-4">
						<h3 class="animated bounceInDown">Selamat Datang</h3>
							<div class="login-box clearfix animated flipInY">
						<div class="login-logo">
			        		<a href="#"><img src="./taufikpay/img/login-logo.png" alt="Company Logo" /></a>
			        	</div> 
			        	<hr />
			        	<div class="login-form">
					  <div class="time">
					      <span class="jam" id="jam"></span>
					  </div>
						<div class="list">
						</div>
						  $(if error)
						    <div class="alert alert-error">
							<button type="button" class="close" data-dismiss="alert">&times;</button>
							<h3 style="text-align: center;">Kesalahan!</h3>
							<p>$(error)</p>
						    </div>
						  $(endif)
						  <form name="login" action="$(link-login-only)" method="post" $(if chap-id) onSubmit="return doLogin()" $(endif)>
						      $(if chap-id)
									<input name="username" type="text" value="$(username)" autocomplete="off" placeholder="Masukkan Username Anda" required/> 
									<input name="password" type="password" autocomplete="off" placeholder="Masukkan Password Anda" required/> 
									<button type="submit" class="btn btn-login">Login</button> 
									</form>
									<div class="login-links"> 
									$(if trial == 'yes')
							  
							  <br />
						      $(endif)
						      <a href="./panduan-login.html">Panduan Login</a>
						      <br />
						      
						  </div>      		
					  </div> 			        	
			       </div>
			  
				<div class="contact-login row">
			        		<div class="p-contact col-md-12 animated flipInX">
			        			
			        		</div>
			        </div>
			    </div>
			</div>
    	</div>

